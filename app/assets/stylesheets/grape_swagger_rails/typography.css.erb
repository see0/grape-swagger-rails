/* droid-sans-regular - latin */
@font-face {
  font-family: 'Droid Sans';
  font-style: normal;
  font-weight: 400;
  src: url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.eot') %>); /* IE9 Compat Modes */
  src: local('Droid Sans'), local('DroidSans'),
       url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.eot') %>?#iefix) format('embedded-opentype'), /* IE6-IE8 */
       url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.woff2') %>) format('woff2'), /* Super Modern Browsers */
       url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.woff') %>) format('woff'), /* Modern Browsers */
       url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.ttf') %>) format('truetype'), /* Safari, Android, iOS */
       url(<%= font_path('grape_swagger_rails/droid-sans-v6-latin-regular.svg') %>#DroidSans) format('svg'); /* Legacy iOS */
}
